	lx:= 0, lw:= 0, ly:= 0
	#$2="", append number (2, stlenmax), show string(2)
	letter:=first container child(msg)
	#play sound (random(0,2))
	for(pos, 1, string length(10)) do(
	
		set slice visible(letter, ON)
	  	letter:=next sibling(letter)
	  	
	  	for (i, 0,1) do (
